\pard\plain\ltrpar\ql\fi360\s31\sl240\slmult1\itap0{\s31\f1\fs24\lang1033{\*\listtag0}\abinodiroverride\ltrch \tab Mini Bosses -  Mini bosses may appear in the level}{\s31\f1\fs24\lang1033{\*\listtag0}\par}
\pard\plain\ltrpar\ql\fi360\s31\sl240\slmult1\itap0{\s31\f1\fs24\lang1033{\*\listtag0}\par}
